{
  "presets": [
    [
      "env", {
        "targets": {
          "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
        },
        "modules": false
      }
    ],
    "stage-2",
    "react"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-react-remove-prop-types",
    "transform-class-properties",
    "transform-object-assign",
    "transform-object-rest-spread",
    [
      "transform-runtime",
      {
        "polyfill": false
      }
    ],
    [
      "import",
      {
        "libraryName": "antd",
        "style": false
      }
    ]
  ]
}
