  "scripts": {
    "dev": "cross-env NODE_ENV=development node build/build.js",
    "dist": "cross-env NODE_ENV=production node build/build.js",
    "dist:common": "cross-env NODE_ENV=common node build/build.js",
    "dist:esm": "cross-env NODE_ENV=esm node build/build.js",
    "dev:examples": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --config build/webpack.examples.conf.js",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "prettify": "prettier --write src/*.ts src/*.tsx src/**/*.ts src/**/*.tsx examples/src/**/*.ts examples/src/**/*.tsx",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint --project tsconfig.json",
    "lint-staged": "lint-staged",
    "lint-staged:ts": "tsc && node node_modules/tslint/bin/tslint"
  },
  "lint-staged": {
    "components/**/*.tsx": [
      "npm run lint-staged:ts"
    ],
    "{site,components}/**/*.less": "stylelint --syntax less",
    "components/*/demo/*.md": [
      "npm run lint-staged:demo"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ],
  "sideEffects": [
    "lib/**/style/*"
  ],
  "engines": {
    "node": ">= 7.0.0",
    "npm": ">= 4.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
